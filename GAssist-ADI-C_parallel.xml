<algorithm_specification>
  <name>GAssist-Parallel</name>
  <nParameters>44</nParameters>
  <seed>1</seed>
  <nOutput>1</nOutput>
  <parameter>
    <name>hierarchicalSelectionThreshold</name>
    <type>real</type>
    <default>0</default>
  </parameter>
  <parameter>
    <name>iterationRuleDeletion</name>
    <type>integer</type>
    <default>5</default>
  </parameter>
  <parameter>
    <name>iterationHierarchicalSelection</name>
    <type>integer</type>
    <default>24</default>
  </parameter>
  <parameter>
    <name>ruleDeletionMinRules</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>30</upperB>
    </domain>
    <default>12</default>
  </parameter>
  <parameter>
    <name>sizePenaltyMinRules</name>
    <type>integer</type>
    <domain>
      <lowerB>2</lowerB>
      <upperB>10</upperB>
    </domain>
    <default>4</default>
  </parameter>
  <parameter>
    <name>numIterations</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>10000</default>
  </parameter>
  <parameter>
    <name>initialNumberOfRules</name>
    <type>integer</type>
    <default>20</default>
  </parameter>
  <parameter>
    <name>popSize</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>1200</default>
  </parameter>
  <parameter>
    <name>probCrossover</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>probMutationInd</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.6</default>
  </parameter>
  <parameter>
    <name>probOne</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.90</default>
  </parameter>
  <parameter>
    <name>tournamentSize</name>
    <type>integer</type>
    <default>3</default>
  </parameter>
  <parameter>
    <name>discretizer1</name>
    <type>text</type>
    <default>UniformWidth_4</default>
  </parameter>
  <parameter>
    <name>discretizer2</name>
    <type>text</type>
    <default>UniformWidth_5</default>
  </parameter>
  <parameter>
    <name>discretizer3</name>
    <type>text</type>
    <default>UniformWidth_6</default>
  </parameter>
  <parameter>
    <name>discretizer4</name>
    <type>text</type>
    <default>UniformWidth_7</default>
  </parameter>
  <parameter>
    <name>discretizer5</name>
    <type>text</type>
    <default>UniformWidth_8</default>
  </parameter>
  <parameter>
    <name>discretizer6</name>
    <type>text</type>
    <default>UniformWidth_10</default>
  </parameter>
  <parameter>
    <name>discretizer7</name>
    <type>text</type>
    <default>UniformWidth_15</default>
  </parameter>
  <parameter>
    <name>discretizer8</name>
    <type>text</type>
    <default>UniformWidth_20</default>
  </parameter>
  <parameter>
    <name>discretizer9</name>
    <type>text</type>
    <default>UniformWidth_25</default>
  </parameter>
  <parameter>
    <name>discretizer10</name>
    <type>text</type>
    <default>Disabled</default>
    <disabled/>
  </parameter>
  <parameter>
    <name>maxIntervals</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>5</default>
  </parameter>
  <parameter>
    <name>probSplit</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.05</default>
  </parameter>
  <parameter>
    <name>probMerge</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.05</default>
  </parameter>
  <parameter>
    <name>probReinitializeBegin</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.03</default>
  </parameter>
  <parameter>
    <name>probReinitializeEnd</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0</default>
  </parameter>
  <parameter>
    <name>adiKR</name>
    <type>list</type>
    <domain>
      <item>false</item>
      <item>true</item>
    </domain>
    <disabled/>
    <default>true</default>
  </parameter>
  <parameter>
    <name>useMDL</name>
    <type>list</type>
    <domain>
      <item>false</item>
      <item>true</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>iterationMDL</name>
    <type>integer</type>
    <domain>
      <lowerB>5</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>25</default>
  </parameter>
  <parameter>
    <name>initialTheoryLengthRatio</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.075</default>
  </parameter>
  <parameter>
    <name>weightRelaxFactor</name>
    <type>real</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1</upperB>
    </domain>
    <default>0.90</default>
  </parameter>
  <parameter>
    <name>defaultClass</name>
    <type>list</type>
    <domain>
      <item>auto</item>
      <item>major</item>
      <item>minor</item>
      <item>disabled</item>
    </domain>
    <default>major</default>
  </parameter>
  <parameter>
    <name>initMethod</name>
    <type>list</type>
    <domain>
      <item>smart</item>
      <item>cwInit</item>
      <item>random</item>
    </domain>
    <default>cwInit</default>
  </parameter>
  <parameter>
  <name>parallelParts</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>3</default>
  </parameter>
    <parameter>
    <name>windowingMethod</name>
    <type>list</type>
    <domain>
      <item>Standard (SCV)</item>
      <item>DOB-SCV</item>
    </domain>
    <default>√DOB-SCV√</default>
  </parameter>
  <parameter>
  <name>fixedSubPopSize</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>0</default>
  </parameter>
    <parameter>
    <name>rotationInterval</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1000000</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>migrationInterval</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1000000</upperB>
    </domain>
    <default>5000</default>
  </parameter>
  <parameter>
    <name>individualsToMigrate</name>
    <type>integer</type>
    <domain>
      <lowerB>1</lowerB>
      <upperB>100</upperB>
    </domain>
    <default>1</default>
  </parameter>
  <parameter>
    <name>evaluateAfterMigration</name>
    <type>list</type>
    <domain>
      <item>false</item>
      <item>true</item>
    </domain>
    <default>false</default>
  </parameter>
  <parameter>
    <name>saveStatisticsEveryXGeneration</name>
    <type>integer</type>
    <domain>
      <lowerB>0</lowerB>
      <upperB>1000</upperB>
    </domain>
    <default>0</default>
  </parameter>
  <parameter>
    <name>initializeRulesLocally</name>
    <type>list</type>
    <domain>
      <item>false</item>
      <item>true</item>
    </domain>
    <default>true</default>
  </parameter>
  <parameter>
    <name>eliteStrategy</name>
    <type>list</type>
    <domain>
      <item>none</item>
      <item>onePerCore</item>
      <item>onePerSet</item>
    </domain>
    <default>onePerSet</default>
  </parameter>
</algorithm_specification>
